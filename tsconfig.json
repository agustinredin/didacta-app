{
    "compilerOptions": {
        "target": "esnext",
        "useUnknownInCatchVariables": false,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "skipLibCheck": true,
        "checkJs": true,
        "esModuleInterop": true,
        "allowJs": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "types": [
            "vite/client"
        ],
        "rootDir": "src",
        "outDir": "dist",
        "noEmit": false
    },
    "include": [
        "src",
        "env.d.ts"
    ]
}